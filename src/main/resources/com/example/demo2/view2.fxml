            <Label layoutX="466.0" layoutY="12.0" text="Проверка">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TextField fx:id="textfield1" layoutX="391.0" layoutY="173.0" prefHeight="25.0" prefWidth="242.0" />
