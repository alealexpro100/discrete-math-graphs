                      <MenuItem mnemonicParsing="false" onAction="#onTo11" style="-fx-font: normal 20px 'serif';" text="11" />
                      <MenuItem mnemonicParsing="false" onAction="#onTo12" style="-fx-font: normal 20px 'serif';" text="12" />
                    </items>
                  </Menu>
            <MenuItem mnemonicParsing="false" onAction="#onClickExit" text="Quit" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button layoutX="14.0" layoutY="66.0" mnemonicParsing="false" onAction="#onClickUpdateTable" text="Обновить таблицу">
               <font>
                  <Font size="13.0" />
               </font></Button>
            <TableView fx:id="tableview1" editable="true" layoutX="14.0" layoutY="116.0" prefHeight="310.0" prefWidth="310.0" />
